# Test Environment Configuration
# Used during automated testing - DO NOT use production values

# Environment
NODE_ENV=test

# Test Spreadsheet IDs (use dedicated test spreadsheets)
PROGRAM_SPREADSHEET_ID=test-program-spreadsheet-id
DELIVERABLES_SPREADSHEET_ID=test-deliverables-spreadsheet-id
SUBCONTRACT_SPREADSHEET_ID=test-subcontract-spreadsheet-id
COMPLIANCE_SPREADSHEET_ID=test-compliance-spreadsheet-id
FINANCIAL_SPREADSHEET_ID=test-financial-spreadsheet-id

# Test Server URLs (ports 13001-13005 for test servers)
PROGRAM_SERVER_URL=http://localhost:13001
DELIVERABLES_SERVER_URL=http://localhost:13002
SUBCONTRACT_SERVER_URL=http://localhost:13003
COMPLIANCE_SERVER_URL=http://localhost:13004
FINANCIAL_SERVER_URL=http://localhost:13005

# Google OAuth Credentials (test credentials only)
CREDENTIALS_PATH=./test-credentials.json
TOKEN_PATH=./test-token.json

# LLM Configuration (disabled for unit tests, can enable for integration tests)
LLM_ENABLED=false
ANTHROPIC_API_KEY=test-api-key
OPENAI_API_KEY=test-api-key
GOOGLE_AI_API_KEY=test-api-key

# External Services (disabled for tests)
DISABLE_EXTERNAL_SERVICES=true

# Test Timeouts
TEST_TIMEOUT=30000

# Coverage Thresholds
COVERAGE_GLOBAL_LINES=80
COVERAGE_GLOBAL_BRANCHES=75
COVERAGE_CRITICAL_LINES=90
COVERAGE_CRITICAL_BRANCHES=85

# Test Data
TEST_PROGRAM_ID=PROG-TEST-001
TEST_USER_EMAIL=test@example.com
